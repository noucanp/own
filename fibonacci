#include <stdio.h>
int f(int n);
int a[999]={0};
int main()
{
	int n=0;
	scanf("%d",&n);
	int a=f(n);
	printf("%d",a);
}
int f(int n)
{
	if(n==1){
		return 1;
	}else if(n==2){
		return 2;
	}else if(n==3){
		return 4;
	}else if(a[n]!=0){
		return a[n];
	}else{
		a[n]=f(n-1)+f(n-2)+f(n-3);
		return f(n-1)+f(n-2)+f(n-3);
	}
}
